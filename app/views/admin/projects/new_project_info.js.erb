var d = dialog({
  id: 'add_project_info',
  title: '添加资料',
  width: 800,
  height: 450,
  content: "<%= j(render partial: 'project_info_form')  %>",
  okValue: '确定',
    ok: function () {
      if(!$("#project_info_form").validationEngine("validate")){
        console.log('form  error')
        return false;
       }
      this.title('提交中…');
      var form = document.querySelector('#project_info_form');
      $(".ui-dialog-button button[i-id=ok]").attr("disabled",true);
      Rails.fire(form, 'submit');
      return false
    },
    cancelValue: '取消',
    cancel: function () {}
});
d.showModal();