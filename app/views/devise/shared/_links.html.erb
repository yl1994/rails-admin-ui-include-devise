<%- if controller_name != 'sessions' %>
  <div class="text-center">
    <%= link_to "登录", new_session_path(resource_name),class: 'small' %>
  </div>
<% end %>
<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <div class="text-center">
    <%= link_to "注册", new_registration_path(resource_name),class: 'small' %>
  </div>
<% end %>